    <script>
        var drawCats1 = function (howManyTimes) {
    for (var i = 0; i < howManyTimes; i++) {
        document.write(i + " =^.^=" + "<br>");
    }
};

var drawCats = prompt("Введи какое количество катеек хочешь увидеть?", "0");
drawCats = +drawCats;
drawCats1(drawCats);
    </script>
